FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      psi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    elecCharge
    {
	type            fixedValue;
	value           uniform 0.1034;      
    }
   
    elecNeutral
    {
	type            fixedValue;
        value           uniform -0.1034;        
    }
    
    Sides
    {
	type            empty;     
    }    
}

