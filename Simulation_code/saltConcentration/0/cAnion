FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      ciAnion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 -3 0 0 1 0 0 ];

internalField   uniform 9.48946E-05;

boundaryField
{
    elecNorth
    {
        type            zeroIonicFlux;
        value           uniform 9.48946E-05;       
    }
   
    elecSouth
    {
        type            zeroIonicFlux;
        value           uniform 9.48946E-05;      
    }
    
    Sided
    {
	type            empty;     
    }
}

