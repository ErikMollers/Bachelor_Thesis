FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    elecCharge
    {
	type            fixedValue;
	value           uniform (0 0 0);    
    }
   
    elecNeutral
    {
	type            fixedValue;
	value           uniform (0 0 0);     
    }

    Sides
    {
	type            fixedValue;
	value		uniform (0 0 0);     
    }
}

